{
  "name": "python-snippet-generator",
  "displayName": "Python Snippet Generator",
  "description": "Python code snippet generator with autocompletion",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Snippets",
    "Other"
  ],
  "activationEvents": [
    "onLanguage:python"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
        {
            "command": "python-snippet-generator.insertSnippet",
            "title": "Insert Python Snippet",
            "category": "Python Snippets"
        }
    ],
    "menus": {
        "command-palette": [
            {
                "command": "python-snippet-generator.insertSnippet"
            }
        ],
        "editor/context": [
            {
                "command": "python-snippet-generator.insertSnippet",
                "group": "navigation",
                "when": "editorLangId == python"
            }
        ]
    }
},
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.105.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.9.0",
    "typescript": "^5.6.2",
    "@vscode/test-electron": "^2.4.1"
  }
}